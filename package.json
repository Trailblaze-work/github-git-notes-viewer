{
  "name": "github-git-notes-viewer",
  "version": "1.0.0",
  "private": true,
  "description": "Browser extension to display git notes inline on GitHub commit pages.",
  "scripts": {
    "test": "node test/unit.mjs && node test/e2e.mjs",
    "test:unit": "node test/unit.mjs",
    "test:e2e": "node test/e2e.mjs",
    "screenshots": "node test/screenshot.mjs",
    "build": "scripts/build-zip.sh",
    "version": "node -e \"const fs=require('fs');const m=JSON.parse(fs.readFileSync('manifest.json','utf8'));m.version=require('./package.json').version;fs.writeFileSync('manifest.json',JSON.stringify(m,null,2)+'\\n');\" && git add manifest.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "puppeteer": "^24.37.5"
  }
}
